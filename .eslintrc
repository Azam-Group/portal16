{
  "extends": "eslint:recommended",
  "plugins": [],
  "env": {
    "node": true
  },
  "globals": {
    "module": true,
    "rootRequire": true,
    "Set": true,
    "Promise": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used"
      }
    ]
  }
}
