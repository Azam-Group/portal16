.wrapperHorizontalStripes {
    color: black;
    > section {
        lost-utility: clearfix;
        padding 20px 0

        header {
            margin-bottom 10px
            h1 {
                font-size: 2.6em;
                line-height: 1.2em;
            }
        }
        .licences {
            .cc {
                font-size 30px
            }
        }
    }
    > section:nth-child(even) {
        background-color: #f9f9f9;
    }
    > section:nth-child(odd) {
        background-color: #ffffff;
    }
    .centerContent {
        max-width: 980px;
        margin-right: auto;
        margin-left: auto;
        .two-third {
            lost-column: 2/3;
        }
        .one-third {
            lost-column: 1/3;
        }
    }
    .mapSection {
        height vr-block(25)
        padding 0 0
    }
    .mapBlock {
        height 100%
    }
    ul, ol {
        margin-bottom 1.5rem
    }
    .licences {
        text-align right
        img {
            float right
        }
    }
    a {
        color $link-color
    }

}