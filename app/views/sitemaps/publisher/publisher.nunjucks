<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in pages.results %}
        <url>
            <loc>{$ _meta.domain $}/publisher/{$ page.key $}</loc>
            <changefreq>weekly</changefreq>
            <priority>0.5</priority>
        </url>
    {% endfor %}
</urlset>