gbmap {
    display block
    overflow hidden
    > div {
        width 100%
        height 100%
        position relative
        .map-area {
            width 100%
            height 100%
        }
        .globe-context {
            position absolute
            top 0
            right 0
            z-index 10
            width 70px
            height 70px
        }
    }
}