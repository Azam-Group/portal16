<div class="container--desktop m-t-1 ng-cloak" ng-if="publisherKey.withCoordinates.count > 0">
    <h4 class="text-muted small text-uppercase">{{ publisherKey.withCoordinates.count | localNumber }}  georeferenced records</h4>
    <div class="map__lockedRatio card">
        <div ng-if="publisherKey.key">
            <gbmap publishing-org="publisherKey.key" mapstyle="'light'"></gbmap>
        </div>
    </div>
</div>