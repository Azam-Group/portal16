<div class="container--desktop m-t-1 ng-cloak" ng-if="datasetKey2.capabilities.total > 0">
    <h4 class="text-muted small text-uppercase">{{ datasetKey2.withCoordinates.count | localNumber }}  georeferenced records</h4>
    <div class="card">
        <div ng-if="datasetKey2.key">
            {#<gbmap dataset-key="datasetKey2.key" mapstyle="'light'"></gbmap>#}
            <map-widget filter="{dataset_key:datasetKey2.key}" style="{suggested: 'DOTS'}"></map-widget>
        </div>
    </div>
</div>