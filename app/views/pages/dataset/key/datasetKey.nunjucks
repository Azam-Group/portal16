{% extends "../../../shared/layout/html/html.nunjucks" %}

{% block page %}

    <script>
        var dataset = {$ dataset | rawJson | safe $};
    </script>

    <article class="datasetKey">
        <p>Title: {$ dataset.record.title $}</p>
        <p>Publisher: {$ dataset.publisher.title $}</p>
        <p>installation: {$ dataset.installation.title $}</p>
    </article>
{% endblock %}

