{%  if results.articles and results.articles.data.length > 0 %}
<section class="card cmsContent">
    <h2>Articles</h2>
        <ul>
            {% for article in results.articles.data | limit(5) %}
                <li>
                    {$ article.title $}
                </li>
            {% endfor %}
        </ul>
</section>
{% endif %}