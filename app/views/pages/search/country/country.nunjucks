{% import "../searchResults.macro.nunjucks" as searchMacro %}
{% if results.country %}

<section class="SearchResults__section SearchResults-catalogNumber">
    <h2>Country</h2>

    {% call searchMacro.simpleCard('/country/' + results.country.country) -%}
    <h3>{{ results.country.title }}</h3>
    <p>{{ results.country.participationStatus }}</p>
    {%- endcall %}

</section>
{% endif %}