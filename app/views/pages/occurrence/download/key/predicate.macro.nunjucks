{% macro print(p, __) %}
    {% if p.type == 'and' or p.type == 'or' %}
        <ul>
            <div class="join">
                <span class="node">{$ p.type | capitalize $}</span>
                <span class="discreet">{$ __('occurrenceDownload.joinDescriptions.' + p.type) $}</span>
            </div>

            {% for predicate in p.predicates %}
                <li {% if predicate.type == 'or' or predicate.type == 'and' %} class="hasChildren" {% endif %}>
                    <div class="pipe"></div>
                    {$ print(predicate, __) $}
                </li>
            {% endfor %}
        </ul>
    {% elseif p.type == 'not' %}
        <ul class="not">
            <div class="join">
                <span class="node">{$ p.type | capitalize $}</span>
                <span class="discreet">None must be included</span>
            </div>
            <li>
                <div class="pipe"></div>
                {$ print(p.predicate, __) $}
            </li>
        </ul>
    {% elseif p.type == 'in' %}
        <div class="leaf">
            <span class="node">{$ __('filterNames.' + (p.key | toCamelCase)) | capitalize $}</span>
            <ol class="inline-bullet-list">
                {% for option in p.values %}
                    <li class="node-value" tooltip-placement="bottom"
                                       tooltip-popup-delay="200"
                                       uib-tooltip="OR - at least one must apply"><span>{$ option | capitalize $}</span></li>
                {% endfor %}
            </ol>
        </div>
    {% elseif p.type == 'within' %}
        <div class="leaf">
            <span class="node">{$ __('filterNames.' + (p.key | toCamelCase)) | capitalize $}</span>
            <span class="node-value">{$ p.geometry | capitalize $}</span>
        </div>
    {% else %}
        <div class="leaf">
            <span class="node">{$ __('filterNames.' + (p.key | toCamelCase)) | capitalize $}</span>
            <span class="node-value">{$ p.value | capitalize $}</span>
        </div>
    {% endif %}
{% endmacro %}

